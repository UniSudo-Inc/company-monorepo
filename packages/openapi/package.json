{
  "name": "@company/openapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.lib.json",
    "codegen:users": "openapi-rq -i http://localhost:8787/doc -o ./src/users --operationId --enums typescript",
    "dev": "rm -rf dist && tsc -p tsconfig.lib.json --watch",
    "format": "prettier --write",
    "lint": "eslint --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "5.59.14"
  },
  "devDependencies": {
    "@7nohe/openapi-react-query-codegen": "1.6.1",
    "@company/configs": "workspace:*"
  }
}
